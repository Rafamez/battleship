            <WrapPanel>
                <Label x:Name="scoreLabel" Content="Score" Width="60" Height="25" VerticalAlignment="Top" HorizontalAlignment="Right" Margin="10 10 10 10" FontFamily="BatmanForeverAlternate"/>
                <TextBlock x:Name="_Score" Text="0" Width="80" Height="20" VerticalAlignment="Top" HorizontalAlignment="Right" Margin="0 10 5 10" Padding="4" FontFamily="BatmanForeverAlternate"/>

                <Label x:Name="TimeLabel" Content="Time" Width="auto" Height="20" VerticalAlignment="Top" HorizontalAlignment="Right" Margin="10 10 10 10" FontFamily="BatmanForeverAlternate"/>
                <TextBlock x:Name="_Time" Text="0" Width="43" Height="20" VerticalAlignment="Top" HorizontalAlignment="Right" Margin="0 10 5 10" Padding="3" FontFamily="BatmanForeverAlternate"/>

                <Label x:Name="Difficulty" Content="Difficulty" Width="auto" Height="22" VerticalAlignment="Top" HorizontalAlignment="Right" Margin="10 10 10 10" FontFamily="BatmanForeverAlternate"/>
                <Button x:Name="Easy" Content="Easy" FontFamily="BatmanForeverAlternate"  Width="65" Click="Easy_Click"/>
                <Button x:Name="Medium" Content="Medium" FontFamily="BatmanForeverAlternate" Width="65" Click="Medium_Click"/>
                <Button x:Name="Hard" Content="Hard" FontFamily="BatmanForeverAlternate" Width="65" Click="Hard_Click"/>

