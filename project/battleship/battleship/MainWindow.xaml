        <ToolBar x:Name="menu" Background="White">
            <Menu>
                <MenuItem x:Name="Languages" Header="Language">
                    <MenuItem x:Name="FrenchLanguage"  Header="French" Click="French" />
                    <MenuItem x:Name="EnglishLanguage" Header="English" Click="English"/>
                </MenuItem>
                <MenuItem x:Name="Cheats" Header="Cheats">
                    <MenuItem x:Name="On" Header="On" Icon="" Checked="CheatsOn" IsCheckable="True"/>
                    <MenuItem x:Name="Off" Header="Off" Icon=""  Checked="CheatsOff" IsCheckable="True"/>
                </MenuItem>
                <MenuItem x:Name="Skins" Header="Skin" RenderTransformOrigin="0.414,0.957">
                    <MenuItem x:Name="UnitedStates" Header="USA"   Click="USA">
                        <MenuItem.Icon>
                            <Image Source="Images\USA.png" Width="20" Height="20" />
                        </MenuItem.Icon>
                    </MenuItem>
                    <MenuItem x:Name="Japan" Header="JPN" Click="JPN">
                        <MenuItem.Icon>
                            <Image Source="Images\Japan.png" Width="20" Height="20" />
                        </MenuItem.Icon>
                    </MenuItem>
                    <MenuItem x:Name="Russia" Header="RUS" Click="RUS">
                        <MenuItem.Icon>
                            <Image Source="Images\Russia.jpg" Width="20" Height="20" />
                        </MenuItem.Icon>
                    </MenuItem>
                    <MenuItem x:Name="Germany" Header="GER" Click="GER">
                        <MenuItem.Icon>
                            <Image Source="Images\Germany.jpg" Width="20" Height="20" />
                        </MenuItem.Icon>
                    </MenuItem>
                </MenuItem>

            </Menu>
        </ToolBar>


        <Label x:Name="Title" Content="Battleship: The Game" VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="24"/>


        <Border BorderBrush="Black" BorderThickness="2" Height="44" Margin="0,0,-0.8,0">
            <WrapPanel Margin="0,0,0,-2" RenderTransformOrigin="0.5,0.5">
                <WrapPanel.RenderTransform>
                    <TransformGroup>
                        <ScaleTransform/>
                        <SkewTransform AngleX="1.468"/>
                        <RotateTransform/>
                        <TranslateTransform X="0.474"/>
                    </TransformGroup>
                </WrapPanel.RenderTransform>
                <Label x:Name="scoreLabel" Content="Score" Width="56.028" Height="24" VerticalAlignment="Top" HorizontalAlignment="Right" Margin="10 10 10 10"/>
                <TextBlock x:Name="_Score" Text="0" Width="80" Height="20" VerticalAlignment="Top" HorizontalAlignment="Right" Margin="0 10 5 10" Padding="4" />

                <Label x:Name="TimeLabel" Content="Time" Width="40.677" Height="26" VerticalAlignment="Top" HorizontalAlignment="Right" Margin="10 10 10 10" />
                <TextBlock x:Name="_Time" Text="0" Width="43" Height="20" VerticalAlignment="Top" HorizontalAlignment="Right" Margin="0 10 5 10" Padding="3"/>
                <Label x:Name="Difficulty" Content="Difficulty" Width="58.852" Height="24.999" VerticalAlignment="Top" HorizontalAlignment="Right" Margin="10 10 10 10" />
                <Button x:Name="Easy" Content="Easy"  Width="65" Click="Easy_Click"/>
                <Button x:Name="Medium" Content="Medium" Width="65" Click="Medium_Click"/>
                <Button x:Name="Hard" Content="Hard" Width="65" Click="Hard_Click" Margin=" 0 0 30 0"/>
                <Label x:Name="Credits" Content="Credits" Width="58.852" Height="24.999" VerticalAlignment="Top" HorizontalAlignment="Right" Margin="10 10 10 10" Visibility="Hidden"/>
                <TextBlock x:Name="CreditsValue" Text="" Width="72" Height="20" VerticalAlignment="Top" HorizontalAlignment="Right" Margin="0 10 5 10" Padding="3" Visibility="Hidden"/>

            </WrapPanel>
